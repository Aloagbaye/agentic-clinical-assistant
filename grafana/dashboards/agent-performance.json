{
  "dashboard": {
    "title": "Agent Performance",
    "tags": ["agentic-clinical-assistant", "agents"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Agent Runs by Status",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(agent_runs_total[5m])) by (status)",
            "legendFormat": "{{status}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Agent Step Latency (p50, p95, p99)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(agent_step_latency_ms_bucket[5m])) by (le, step))",
            "legendFormat": "p50 {{step}}"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(agent_step_latency_ms_bucket[5m])) by (le, step))",
            "legendFormat": "p95 {{step}}"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(agent_step_latency_ms_bucket[5m])) by (le, step))",
            "legendFormat": "p99 {{step}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "yaxes": [
          {"format": "ms", "label": "Latency (ms)"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "Tool Call Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum(rate(tool_calls_total[5m])) by (tool_name)",
            "legendFormat": "{{tool_name}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Tool Call Latency by Tool",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(tool_call_latency_ms_bucket[5m])) by (le, tool_name))",
            "legendFormat": "p95 {{tool_name}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "yaxes": [
          {"format": "ms", "label": "Latency (ms)"},
          {"format": "short"}
        ]
      }
    ]
  }
}

